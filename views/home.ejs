<!-- Javascript for news ticker -->

    <script>
        function showNext(current)
        {
            switch(current){
                case 'news1': $("#news1").hide(); $("#news2").show(); break;
                case 'news2': $("#news2").hide(); $("#news3").show(); break;
                case 'news3': alert("End"); break;
            }
        }

        function showPrev(current)
        {
            switch(current){
                case 'news1': alert("End"); break;
                case 'news2': $("#news2").hide(); $("#news1").show(); break;
                case 'news3': $("#news3").hide(); $("#news2").show(); break;
            }
        }

        $(".more_link").mouseover(function(){
            $(this).css("color","black");
            $(this).mouseleave(function(){
            $(this).css("color","#182747");
            });
        });  
    </script>



  


    
